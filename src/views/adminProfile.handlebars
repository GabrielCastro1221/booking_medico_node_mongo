<link rel="stylesheet" href="/css/admin/admin.css" />

<section class="section-main mt-13">
    <div class="container">
        <div id="content" class="grid">
            <div class="tabs-btns">
                <button class="group-1" data-target="users">Usuarios</button>
                <button class="group-1" data-target="doctors">Doctores</button>
                <button class="group-1" data-target="citas">Citas</button>
                <button class="group-1" data-target="tickets">Tickets</button>
            </div>
        </div>
        <div class="tab-content" id="users">{{> adminUsers}}</div>
        <div class="tab-content" id="doctors">{{> adminDoctors}}</div>
        <div class="tab-content" id="citas">{{> adminCitas}}</div>
        <div class="tab-content" id="tickets">{{> adminTickets}}</div>
    </div>
</section>

<script src="/js/admin/admin.js"></script>